{
    "name": "react-app",
    "version": "0.1.0",
    "private": true,
    "homepage": "http://localhost:5000",
    "dependencies": {
        "antd": "^4.8.2",
        "axios": "^0.21.1",
        "image-file-to-base64-exif": "^1.0.3",
        "moment": "2.24.0",
        "query-string": "^6.9.0",
        "react": "^16.13.1",
        "react-dom": "^16.11.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.4.0"
    },
    "scripts": {
        "start": "source env/local_env.sh && react-scripts start",
        "dev": "env-cmd -f env/local.env react-scripts start",
        "build": "env-cmd -f env/build.env react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app",
        "env": {
            "es6": true
        },
        "ignorePatterns": [
            "node_modules/"
        ],
        "parser": "babel-eslint",
        "parserOptions": {
            "ecmaVersion": 8
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "env-cmd": "^10.1.0"
    }
}
